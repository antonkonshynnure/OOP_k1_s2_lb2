#include <iostream>
#include <math.h>
using namespace std;
class Vector
{
private:
    float* body;
    size_t dim;
public:
    Vector() {
        body = new float[2];
        dim = 2;
    }
    Vector(size_t n) {
        body = new float[n];
        dim = n;
    };
    Vector(Vector& v) {
        body = v.body;
        dim = v.dim;
    }
    Vector(Vector&& v) {
        body = v.body;
        dim = v.dim;
    }
    Vector(float* arr, size_t d) {
        body = new float[d];
        dim = d;
        for (size_t i = 0; i < dim; i++)
            body[i] = arr[i];
    }
    float mod() {
        float s = 0.0;
        for (size_t i = 0; i < dim; i++)
            s += body[i] * body[i];
        return sqrtf(s);
    }
    Vector operator=(Vector& v) {
        body = v.body;
        dim = v.dim;
        return *this;
    }
    Vector operator=(Vector&& v) {
        body = v.body;
        dim = v.dim;
        return *this;
    }
    float operator[](int i) {
        return body[i];
    }
    Vector operator+(Vector v) {
        if (dim == v.dim) {
            Vector res(dim);
            for (size_t i = 0; i < dim; i++)
                res.body[i] = body[i] + v[i];
            return res;
        }
        abort();
    }
    Vector operator-(Vector v) {
        if (dim == v.dim) {
            Vector res(dim);
            for (size_t i = 0; i < dim; i++)
                res.body[i] = body[i] - v[i];
            return res;
        }
        abort();
    }
    Vector operator*(float r) {
        Vector res(dim);
        for (size_t i = 0; i < dim; i++)
            res.body[i] = r * body[i];
        return res;
    }
    float operator*(Vector v) {
        if (dim == v.dim) {
            float s = 0.0;
            for (size_t i = 0; i < dim; i++)
                s += body[i] * v.body[i];
            return s;
        }
        abort();
    }
    bool operator==(Vector v) {
        if (dim == v.dim) {
            bool res = true;
            for (size_t i = 0; i < dim; i++)
                res = res && (body[i] == v.body[i]);
            return res;
        }
        else {
            return false;
        }
    }
    bool operator!=(Vector v) {
        return !((*this) == v);
    }
    bool operator>(Vector v) {
        return mod() > v.mod();
    }
    bool operator<(Vector v) {
        return mod() < v.mod();
    }
    void operator>>(FILE* f) {
        fprintf(f, "(");
        for (size_t i = 0; i < dim; i++)
            fprintf(f, "%f ", body[i]);
        fprintf(f, ")\n");
    }
    void operator<<(FILE* f) {
        for (size_t i = 0; i < dim; i++)
            fscanf(f, "%f", &body[i]);
    }
};
int main()
{
    Vector v0;
    Vector v1(4);
    Vector v2(4);
    v0 << stdin;
    v1 << stdin;
    v2 << stdin;
    v0 >> stdout;
    v1 >> stdout;
    v2 >> stdout;
    Vector v3 = v1 + v2;
    Vector v4 = v1 - v2;
    Vector v5 = v4 * (2.0);
    Vector v6(v5);
    v3 >> stdout;
    v4 >> stdout;
    v5 >> stdout;
    float arr[4] = { 1.1, 2.2, 3.4, 6.88 };
    Vector v7(arr, 4);
    cout << v7[2] << endl;
    cout << v6[2] << endl;
    cout << v1 * v2 << endl;
    cout << (v1 == v1) << endl;
    cout << (v1 != v2) << endl;
    cout << v1.mod() << endl;
    cout << v2.mod() << endl;
    cout << (v1 > v2) << endl;
    cout << (v2 < v1) << endl;
}
